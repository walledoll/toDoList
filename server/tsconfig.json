{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "ES2020",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "preserveConstEnums": true
  },
  "include": ["src/**/*"]
}